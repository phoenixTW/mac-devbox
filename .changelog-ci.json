{
  "changelog_type": "pull_request",
  "header_prefix": "##",
  "commit_changelog": true,
  "comment_changelog": false,
  "include_unlabeled_changes": true,
  "unlabeled_group_title": "Other Changes",
  "pull_request_title_regex": ".*",
  "version_regex": "v?([0-9]{1,2})+[.]+([0-9]{1,2})+[.]+([0-9]{1,2})",
  "exclude_labels": ["bot", "dependabot", "ci", "automated"],
  "group_config": [
    {
      "title": "Bug Fixes",
      "labels": ["bug", "bugfix", "fix"]
    },
    {
      "title": "New Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "Documentation Updates",
      "labels": ["docs", "documentation", "doc"]
    },
    {
      "title": "Code Improvements",
      "labels": ["improvements", "refactor", "chore"]
    },
    {
      "title": "Security Updates",
      "labels": ["security"]
    },
    {
      "title": "Performance Improvements",
      "labels": ["performance"]
    }
  ]
}
